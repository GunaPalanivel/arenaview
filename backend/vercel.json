{
  "version": 2,
  "buildCommand": "npm run build && npx prisma generate",
  "outputDirectory": ".",
  "installCommand": "npm install",
  "builds": [
    {
      "src": "dist/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/dist/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "VERCEL": "1"
  }
}
